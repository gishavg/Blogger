$(".show").html("<%=escape_javascript (render 'users/showpost',  posts: @posts, user: @user)  %>");
y = $('.pagination li a')
y.each(function( index ) {
  console.log("asdas")
  y[index].setAttribute("data-remote",true)
  url = y[index].getAttribute("href")
  page=""
  if (url.includes('?')){
    page = url.split("?")[1]
  }
  url = "/users/<%=@user.id %>/<%=@type%>?" + page
  console.log(url)
  y[index].setAttribute("href",url)

});

